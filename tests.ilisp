(load "standard.ilisp")

(defn '(assert-eq) '(value expected)
  '(if (== value expected)
    '(true)
    '(error
       (concat
         "Value "
         (str value)
         " did not eq "
         (str expected)))))

; str
(assert-eq (str join) "")
(assert-eq (str 0) "0")
; (assert-eq (str 1.1) "1.1")
